<%-include("partials/header");%>
<div class="container">
    <h1>Edit Your Info:</h1>

<form action="/contact/<%=user._id%>?_method=PUT" method="POST">
    <div class="form-group index_input1">
        <input  type ="text" class="form-control" name="profile[name]" value="<%=user.name%>">
    </div>
    <div class="form-group index_input1">
        <input class="form-control" type="text" name="profile[number]" value="<%=user.number%>">
    </div>
    <div class="index_input1">
        <button class="btn btn-block btn-primary">Submit</button>
    </div>
    
</form>

</div>
<%-include("partials/footer");%>